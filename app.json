{
  "expo": {
    "name": "Pole Vault Tracker 1.0",
    "slug": "polevault-tracker2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/pole-vaulter.png",
    "splash": {
      "image": "./assets/pole-vaulter.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.parmamanmi.polevaulttracker2",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app uses the camera for recording pole vault videos.",
        "NSMicrophoneUsageDescription": "This app uses the microphone for recording audio during video capture.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to let you upload and view vault videos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/pole-vaulter.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.parmamanmi.polevaulttracker2",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "0a4b22ca-5095-416f-a972-66f143582194"
      }
    },
    "plugins": [
      "expo-mail-composer",
      "expo-video"
    ]
  }
}
